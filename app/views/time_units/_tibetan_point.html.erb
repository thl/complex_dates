<fieldset id="tibetan_point_date" class="">
	<legend style="border:1px solid green; color:white; background: black;">Tibetan date</legend>
	
	<%= hidden_field_tag :type, "tibetan_point" %>

	<%= time_unit_field 'calendrical', 'tibetan_date', ['calendrical', 'calendrical_certainty'], tu_options('month') %>

	<%= time_unit_field 'rabjung', 'tibetan_date', ['rabjung', 'rabjung_certainty'], tu_options('rabjung') %>

	<%= time_unit_field 'element', 'tibetan_date', ['element', 'element_certainty'], tu_options('element') %>

	<%= time_unit_field 'animal', 'tibetan_date', ['animal', 'animal_certainty'], tu_options('animal') %>

	<%= time_unit_field 'gender', 'tibetan_date', ['gender', 'gender_certainty'], tu_options('gender') %>

	<%= time_unit_field 'month', 'tibetan_date', ['tibetan_month', 'tibetan_month_certainty'], tu_options('month') %>

	<%= time_unit_field 'intercalary_month', 'tibetan_date', ['tibetan_intercalary_month', 'tibetan_intercalary_month_certainty'], tu_options('intercalary_month') %>

	<%= time_unit_field 'day', 'tibetan_date', ['tibetan_day', 'tibetan_day_certainty'], tu_options('tibetan_day') %>

	<%= time_unit_field 'intercalary_day', 'tibetan_date', ['tibetan_intercalary_day', 'tibetan_intercalary_day_certainty'], tu_options('intercalary_day') %>

	<%= time_unit_field 'hour', 'tibetan_date', ['tibetan_hour', 'tibetan_hour_certainty'], tu_options('hour') %>

	<%= time_unit_field 'minute', 'tibetan_date', ['tibetan_minute', 'tibetan_minute_certainty'], tu_options('minute') %>

	<%= time_unit_field 'am_pm', 'tibetan_date', ['tibetan_am_pm', 'tibetan_am_pm_certainty'], tu_options('am_pm') %>

	<%= time_unit_field 'time_of_day', 'tibetan_date', ['tibetan_time_of_day', 'tibetan_time_of_day_certainty'], tu_options('time_of_day') %>

</fieldset>

  
