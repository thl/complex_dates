	<%= text_field_tag 'start_date[gregorian_end_year]', {}, {'style'=> 'display:none'}%>
	<%= select_tag 'start_date[gregorian_year_certainty]', options_for_select(certainty)%>

 <script text="javascript">
   Event.observe('start_date_gregorian_year_certainty','change', function() { 
	   if($('start_date_gregorian_year_certainty').value == "Probable" || $('start_date_gregorian_year_certainty').value == "Estimated")
           {
                $('start_date_gregorian_end_year').show();
           }
	   else
           {
		$('start_date_gregorian_end_year').hide();
           }
	   
	 });
 </script>
</p>
