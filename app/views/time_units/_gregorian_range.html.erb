<fieldset id="gregorian_range_date" style="width:420px">
	<legend style="border:1px solid green; color:white; background: black;">Gregorian Date</legend>

<%= hidden_field_tag :type, "gregorian_range" %>

<fieldset id="gregorian_range_start" style="width:150px; float:left;">
