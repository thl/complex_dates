<fieldset class="complex-date-range complex-date-range-display">
  	<legend>
		Tibetan Date Range
		<%= edit_item_link @time_unit %>
		<%= delete_item_link @time_unit %>
	</legend>
<%= render :partial => 'admin/time_units/frequency', :locals => {:frequency => @time_unit.frequency} %>	
	<fieldset class="complex-date">
		<legend>Start</legend>
		<%= render :partial => "admin/tibetan_dates/show_fields", :locals => {:object_name => :start_date} %>
	</fieldset>
	
	<fieldset class="complex-date">
		<legend>End</legend>
		<%= render :partial => "admin/tibetan_dates/show_fields", :locals => {:object_name => :end_date} %>
	</fieldset>

</fieldset>