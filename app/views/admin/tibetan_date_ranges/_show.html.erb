<fieldset class="complex-date-range complex-date-range-display">
  	<legend>
		Tibetan Date Range
		<%= edit_item_link @time_unit %>
		<%= delete_item_link @time_unit %>
	</legend>
<%= render :partial => 'admin/time_units/frequency', :locals => {:frequency => @time_unit.frequency} %>	
	<fieldset class="complex-date">
		<legend>Start</legend>
<%  date = @time_unit.start_date
    if date.nil? %>
      -
<%  else %>
<%=   render partial: 'admin/tibetan_dates/show_fields', locals: {complex_date: date} %>
<%  end %>
	</fieldset>
	<fieldset class="complex-date">
		<legend>End</legend>
<%  date = @time_unit.end_date
    if date.nil? %>
      -
<%  else %>
<%=   render partial: 'admin/tibetan_dates/show_fields', locals: {complex_date: date} %>
<%  end %>
	</fieldset>
</fieldset>