<% form_for @time_unit, :url => {:action => 'create'} do |f| %>

<%= f.hidden_field :calendar_id %>
<%= f.hidden_field :is_range %>

<fieldset class="complex-date-range">
  	<legend>Gregorian Date Range</legend>
	
	<fieldset class="complex-date">
		<legend>Start</legend>
		<%= render :partial => "gregorian_dates/form_fields", :locals => {:f => f, :object_name => :start_date} %>
	</fieldset>
	
	<fieldset class="complex-date">
		<legend>End</legend>
		<%= render :partial => "gregorian_dates/form_fields", :locals => {:f => f, :object_name => :end_date} %>
	</fieldset>

</fieldset>

<% end %>

<%= render :partial => "time_units/remove_form_tag" %>