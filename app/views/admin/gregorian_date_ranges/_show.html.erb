<fieldset class="complex-date-range complex-date-range-display">
  	<legend>Gregorian Date Range</legend>
	
	<fieldset class="complex-date">
		<legend>Start</legend>
		<%= render :partial => "gregorian_dates/show_fields", :locals => {:object_name => :start_date} %>
	</fieldset>
	
	<fieldset class="complex-date">
		<legend>End</legend>
		<%= render :partial => "gregorian_dates/show_fields", :locals => {:object_name => :end_date} %>
	</fieldset>

</fieldset>