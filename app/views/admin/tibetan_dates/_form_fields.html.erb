<% f.fields_for object_name do |date_form| %>
	<% date_form.object = @time_unit.send(object_name) unless @time_unit.send(object_name).nil? %>
	<%= complex_date_select date_form, :calendrical_id, false %>
	<%= complex_date_select date_form, :rabjung_id %>
	<%= complex_date_select date_form, :element_id, false %>
	<%= complex_date_select date_form, :animal_id, false %>
	<%= complex_date_select date_form, :gender_id, false %>
	<%= complex_date_select date_form, :month, true, :collection => date_options_for(:month), :intercalary => :month %>
	<%= complex_date_select date_form, :day, true, :collection => date_options_for(:tibetan_day), :intercalary => :day %>
	<%= complex_date_select date_form, :hour, true, :collection => date_options_for(:hour) %>
	<%= complex_date_select date_form, :minute, true, :collection => date_options_for(:minute) %>
	<%= complex_date_select date_form, :time_of_day_id, true, :collection_text_method => :tibetan_name %>
<% end %>