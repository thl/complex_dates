<fieldset class="complex-date complex-date-display">
  	<legend>
		Tibetan Date
		<%= edit_item_link @time_unit %>
		<%= delete_item_link @time_unit %>
	</legend>
<%= render :partial => 'admin/time_units/frequency', :locals => {:frequency => @time_unit.frequency} %>
<%  date = @time_unit.date
    if !date.nil? %>
<%=   render partial: 'admin/tibetan_dates/show_fields', locals: { complex_date: date } %>
<%  end %>
</fieldset>