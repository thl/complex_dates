<% f.fields_for object_name do |date_form|
     date_form.object = @time_unit.send(object_name) unless @time_unit.send(object_name).nil? %>
<%=  complex_date_text_field date_form, :year %>
<%=  complex_date_select date_form, :month, true, :collection => date_options_for(:month) %>
<%=  complex_date_select date_form, :day, true, :collection => date_options_for(:gregorian_day) %>
<%=  complex_date_select date_form, :day_of_week_id, true, :collection_text_method => :gregorian_name %>
<%=  complex_date_select date_form, :season_id, true, :collection_text_method => :gregorian_name %>
<%=  complex_date_select date_form, :hour, true, :collection => date_options_for(:hour) %>
<%=  complex_date_select date_form, :minute, true, :collection => date_options_for(:minute) %>
<%=  complex_date_select date_form, :time_of_day_id, true, :collection_text_method => :gregorian_name %>
<% end %>