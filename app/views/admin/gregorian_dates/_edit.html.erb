<%= fields_for @time_unit do |f| %>
<div>
<%= f.hidden_field :calendar_id %>
<%= f.hidden_field :is_range %>
<%= f.label :frequency %><br/>
<%= f.collection_select(:frequency_id, GregorianFrequency.options, :id, :title, { :include_blank => true }) %>
</div><br/>
<fieldset class="complex-date">
    <legend>Gregorian Date</legend>
<%= render :partial => "admin/gregorian_dates/form_fields", :locals => {:f => f, :object_name => :date} %>
</fieldset>
<% end %>